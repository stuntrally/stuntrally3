#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3129 ..Editor.Vegetation.Surface.Surface.Text
msgctxt "TipTrailClr"
msgid "Tire trails color. R G B A components 0 to 1 (eg. 0.4 0.4 0.2 0.6)"
msgstr ""
"Farbe der Reifenspuren. R-, G-, B-, A-Komponenten von 0 bis 1 (z.B. 0.4 0.4 "
"0.2 0.6)."

#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3136 ..Editor.Vegetation.Surface.Surface.Combo
msgctxt "TipParType1"
msgid "Tire Particle type 1 (global for track)."
msgstr "Reifenpartikel Typ 1 (ganze Strecke)."

#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3143 ..Editor.Vegetation.Surface.Surface.Combo
msgctxt "TipParType2"
msgid "Tire Particle type 2 (global for track)."
msgstr "Reifenpartikel Typ 2 (ganze Strecke)."

#. TIP  Terrain  particles 
#: data/gui/Editor.layout:3150 ..Editor.Vegetation.Surface.Surface.Combo
msgctxt "TipParType3"
msgid "Tire Particle type 3 (global for track)."
msgstr "Reifenpartikel Typ 3 (ganze Strecke)."

#. TIP  Terrain  particles 
msgctxt "TipTireParType3"
msgid "Tire Particle type 3."
msgstr "Reifenpartikel Typ 3."

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3174 ..Editor.Vegetation.Surface.Surface.Text :3207
#: data/gui/Game_Tweak.layout:325 ..Tweak.Surface.Text :332:350:357
msgctxt "TipBumpFreq"
msgid "Frequency of additional bumps on surface (distance between them)."
msgstr ""
"Frequenz zusätzlicher Unebenheiten der Oberfläche (Distanz zwischen diesen)"

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3178 ..Editor.Vegetation.Surface.Surface.Text :3212
#: data/gui/Game_Tweak.layout:338 ..Tweak.Surface.Text :345:362:369
msgctxt "TipBumpAmp"
msgid "Amplitude of additional bumps on surface (how high)."
msgstr "Amplitude (Höhe) zusätzlicher Unebenheiten der Oberfläche"

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3165 ..Editor.Vegetation.Surface.Surface.Text
#: :3170..Editor.Vegetation.Surface.Surface.Combo
#: :3183..Editor.Vegetation.Surface.Surface.Text :3187
#: data/gui/Game_Tweak.layout:404 ..Tweak.Surface.Text
msgctxt "TipSurfType"
msgid "Type of surface, determines sound."
msgstr "Typ der Oberfläche, bestimmt das Geräusch."

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3198 ..Editor.Vegetation.Surface.Surface.Text :3222
#: data/gui/Game_Tweak.layout:374 ..Tweak.Surface.Text :381
msgctxt "TipRollDrag"
msgid ""
"Drag for wheels, bigger will make them slow down faster (eg. 140 for snow, "
"80 for grass)."
msgstr ""
"Reifenwiderstand, größerer Wert bremst diese stärker ab (z.B. 140 für "
"Schnee, 80 für Gras)"

#. TIP  Terrain  surface 
#: data/gui/Editor.layout:3202 ..Editor.Vegetation.Surface.Surface.Text :3217
#: data/gui/Game_Tweak.layout:288 ..Tweak.Surface.Text :295:300:307:312:319
msgctxt "TipSurfFrict"
msgid "Surface friction (eg. 1.0 asphalt, 0.7 gravel, 0.5 ice)."
msgstr "Oberflächenreibung (z.B. 1.0 Asphalt, 0.7 Kies, 0.5 Eis)"

#. TIP  Vegetation 
#: data/gui/Editor.layout:1893 ..Editor.Grasses.Properties.Text
#: :1897..Editor.Grasses.Properties.Slider
#: :1903..Editor.Grasses.Properties.Edit
msgctxt "TipGrassDens"
msgid "How dense/rare will the grass be on terrain."
msgstr "Wie häufig/selten wird das Gras auf Gelände aufkommen."

#. TIP  Vegetation 
#: data/gui/Editor.layout:2341 ..Editor.Vegetation.Properties.Text
#: :2345..Editor.Vegetation.Properties.Slider
#: :2351..Editor.Vegetation.Properties.Edit
msgctxt "TipTreeDens"
msgid "How many models (trees, rocks, etc.) will be on track."
msgstr "Wie viele Modelle (Bäume, Steine, etc.) auf der Strecke sichtbar sind."

#. TIP  Vegetation 
#: data/gui/Editor.layout:2357 ..Editor.Vegetation.Properties.Text
#: :2361..Editor.Vegetation.Properties.Slider
#: :2366..Editor.Vegetation.Properties.Edit
msgctxt "TipModelRoadDist"
msgid "Distance from road to models (global, added to each model's own value)."
msgstr ""
"Entfernung von der Straße zu den Modellen (global, zu jedem Modell-eigenem "
"Wert addiert)"

#. TIP  Vegetation 
msgctxt "TipImpDist"
msgid "Far distance at which the 2d impostors dissapear."
msgstr "Ferne Distanz, bei der die 2D-Betrüger verschwinden."

#. TIP  Vegetation  page,vis 
msgctxt "TipGrassPageLength"
msgid ""
"Grass Page length (low will reload often, high will cause visible switching)."
msgstr ""
"Page-Länge vom Gras (niedriger Wert verursacht häufiges erneutes Laden, "
"hoher Wert macht die Übergänge sichtbar)"

#. TIP  Vegetation  page,vis 
msgctxt "TipModelPageLength"
msgid ""
"Models Page length (low will reload often and make too many batches, high "
"will cause visible switching and make too many drawn triangles)."
msgstr ""
"Models Page length (niedrig wird häufiges Nachladen verursachen und viele "
"Batches erzeugen, hoch wird sichtbares Switching und zu viele gezeichnete "
"Dreiecke verursachen)"

#. TIP  Vegetation  page,vis 
msgctxt "TipGrassVisDist"
msgid "Visibility distance for grass pages."
msgstr "Sichtbarkeitsentfernung für Grasseiten."

#. TIP  Vegetation  page,vis 
msgctxt "TipModelVisDist"
msgid ""
"Visibility distance for 3d models, after this they turn into 2d impostors."
msgstr ""
"Sichtbarkeitsdistanz für 3D-Modelle, nach der werden sie zu 2D-Betrügern."

#. TIP  Grass 
#: data/gui/Editor.layout:1909 ..Editor.Grasses.Properties.Text
#: :1913..Editor.Grasses.Properties.Slider
#: :1918..Editor.Grasses.Properties.Edit
msgctxt "TipGrDensSmooth"
msgid ""
"Grass density map - smooth size. Higher will make a smoother transition from "
"road to grass."
msgstr ""
"Grasdichten-Karte - Weichzeichnergröße. Ein höherer Wert wird einen "
"weicheren Übergang von Straße zu Gras verursachen."

#. TIP  Grass 
#: data/gui/Editor.layout:2015 ..Editor.Grasses.Grasses.Text
#: :2019..Editor.Grasses.Grasses.Slider :2024..Editor.Grasses.Grasses.Text
#: :2034..Editor.Grasses.Grasses.Slider :2062:2077
msgctxt "TipGrassPatchLengthMin"
msgid "Minimal grass patch length."
msgstr "Minimale Länge eines Grasbüschels."

#. TIP  Grass 
#: data/gui/Editor.layout:2030 ..Editor.Grasses.Grasses.Text :2039
msgctxt "TipGrassPatchLengthMax"
msgid "Maximal grass patch length."
msgstr "Maximale Länge eines Grasbüschels."

#. TIP  Grass 
#: data/gui/Editor.layout:2058 ..Editor.Grasses.Grasses.Text :2067
msgctxt "TipGrassPatchHeightMin"
msgid "Minimal grass patch height."
msgstr "Minimale Höhe eines Grasbüschels."

#. TIP  Grass 
#: data/gui/Editor.layout:2073 ..Editor.Grasses.Grasses.Text :2082
msgctxt "TipGrassPatchHeightMax"
msgid "Maximal grass patch height."
msgstr "Maximale Höhe eines Grasbüschels."

#. TIP  Grass  sway 
msgctxt "TipGrassSwayLen"
msgid "Grass sway effect length (scale)."
msgstr "Grasschwingungslänge (Skalierung)."

#. TIP  Grass  sway 
msgctxt "TipGrassSwaySpd"
msgid "Grass sway effect speed."
msgstr "Grasschwingungsgeschw."

#. TIP  Grass  sway 
msgctxt "TipGrassSwayDistr"
msgid "Grass sway effect distribution (how random)."
msgstr "Grasschwingungsverteilung (wie zufällig)."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2180 ..Editor.Grasses.Channels.Text
#: :2185..Editor.Grasses.Channels.Edit :2189..Editor.Grasses.Channels.Slider
msgctxt "TipGrTerMaxAngle"
msgid ""
"Terrain angle above which there won't be any grass (starts fading out, for "
"smooth angle)."
msgstr ""
"Geländewinkel über welcher wird es kein Gras geben (wird ausblenden für "
"Glätten-Abstand)."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2210 ..Editor.Grasses.Channels.Text
#: :2215..Editor.Grasses.Channels.Edit :2219..Editor.Grasses.Channels.Slider
msgctxt "TipGrTerMinHeight"
msgid ""
"Terrain height below which there won't be any grass (starts fading out, for "
"smooth distance)."
msgstr ""
"Geländehöhe unten welche wird es kein Gras geben (wird ausblenden für "
"Glätten-Abstand)."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2225 ..Editor.Grasses.Channels.Text
#: :2230..Editor.Grasses.Channels.Edit :2234..Editor.Grasses.Channels.Slider
msgctxt "TipGrTerMaxHeight"
msgid ""
"Terrain height above which there won't be any grass (starts fading out, for "
"smooth distance)."
msgstr ""
"Geländehöhe über welche wird es kein Gras geben (wird ausblenden für Glätten-"
"Abstand)."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2088 ..Editor.Grasses.Grasses.Text
#: :2095..Editor.Grasses.Grasses.Button
msgctxt "TipGrassMaterial"
msgid "Grass material (type) for this layer, chosen to match track's scenery."
msgstr ""
"Grasmaterial (Typ) für diese Schicht, gewählt, um zur Szenerie der Strecke "
"zu passen."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:1989 ..Editor.Grasses.Grasses.Text
msgctxt "TipGrassColorMap"
msgid ""
"Grass colormap for scenery, usually same choice as for grass material. "
"Additional grass coloring."
msgstr ""
"Color-Map des Grases, üblicherweise die selbe Auswahl wie für das "
"Grasmaterial. Zusätzliche Graseinfärbung."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:1971 ..Editor.Grasses.Grasses.ProgressBar
#: :2005..Editor.Grasses.Grasses.Text
#: :2154..Editor.Grasses.Channels.ProgressBar
msgctxt "TipGrassLayers"
msgid "Grass layers. Recommended use 1-4."
msgstr "Grasschichten. Empfohlene Verwendung 1-4."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:1983 ..Editor.Grasses.Grasses.Image
msgctxt "TipGrassPrev"
msgid "Preview image of this grass layer (changed with material)."
msgstr "Vorschaubild dieser Grasschicht (wird mit Material verändert)."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2000 ..Editor.Grasses.Grasses.Check
msgctxt "TipToggleGrass"
msgid "Enable or disable this grass layer."
msgstr "Diese Grasschicht aktivieren oder deaktivieren."

#. TIP  Grass  terrain 
#: data/gui/Editor.layout:2101 ..Editor.Grasses.Grasses.Text
#: :2105:2109..Editor.Grasses.Grasses.Slider :2115..Editor.Grasses.Grasses.Text
#: :2119:2123..Editor.Grasses.Grasses.Slider
msgctxt "TipGrassLayerDens"
msgid "Grass density for layer (how often it will appear)."
msgstr "Grasdichte für Schicht (wie oft es auftauchen wird)."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:1937 ..Editor.Grasses.Grasses.TabC
#: :2384..Editor.Vegetation.Models.TabC
#: :2467..Editor.Vegetation.Models.ProgressBar
#: :2505..Editor.Vegetation.Models.Text
msgctxt "TipVegetGroups"
msgid "Vegetation model groups (recommended use 4-6), affects performance."
msgstr "Vegetationsgruppen (empfohlen: 4-6), Einfluss auf Performance."

#. TIP  Vegetation  models 
#: data/gui/Editor.layout:2473 ..Editor.Vegetation.Models.Text
#: :2480..Editor.Vegetation.Models.Button
